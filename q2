2.
#include <iostream>
using namespace std;

int main() {
    int arr[6]; 
    int i,j,z;
    cout << "Enter six values for Array: ";
    for ( i = 0; i <= 5; i++) {  
        cin >> arr[i];
    }
    cout << "The array is :\n ";
    for ( i = 0; i <= 5; i++) {
        cout << arr[i] << " ";
    }
    cout << "array after removing duplicates is: \n";
    for ( i = 0; i <=5; i++) 
    {
       for( j=i+1;j<=5;j++)
       {
           if (arr[i]==arr[j])
           {
              for ( z = j; z < 5; z++)
            {
                
                arr[z] = arr[z + 1]; 
            
            }


           }
       }
       
    }
    for ( i = 0; i <=4; i++) 
    {
        cout << arr[i] ;
    }
  
           
    
    return 0;
}

