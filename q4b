#include <iostream>
using namespace std;

int main() {
   
    int r1, c1, r2, c2;
    
    cout << "Enter rows and columns for first matrix: ";
    cin >> r1 >> c1;
    
    cout << "Enter rows and columns for second matrix: ";
    cin >> r2 >> c2;

    if (c1 != r2) {
        cout << "Can't multiply";
        return 0;
    }

    int first[r1][c1], second[r2][c2], result[r1][c2];

    cout << "\nEnter first matrix (" << r1 << "x" << c1 << "):\n";
    for (int i = 0; i < r1; i++) {
        for (int j = 0; j < c1; j++) {
            cin >> first[i][j];
        }
    }

    cout << "\nEnter second matrix (" << r2 << "x" << c2 << "):\n";
    for (int i = 0; i < r2; i++) {
        for (int j = 0; j < c2; j++) {
            cin >> second[i][j];
        }
    }

    for (int i = 0; i < r1; i++) {
        for (int j = 0; j < c2; j++) {
            result[i][j] = 0;
            for (int k = 0; k < c1; k++) {
                result[i][j] += first[i][k] * second[k][j];
            }
        }
    }

    cout <<" \n" << "Result (" << r1 << "x" << c2 << "):\n";
    for (int i = 0; i < r1; i++) {
        for (int j = 0; j < c2; j++) {
            cout << result[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}

