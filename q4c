#include<iostream>
using namespace std;
int main(){
    int r,c;
    cout<<"Enter number of rows of matrix:";
    cin>>r;
    cout<<"Enter number of colm of matrix:";
    cin>>c;
    int a[r][c];
    cout<<"Enter elements of matrix:";
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cin>>a[i][j];
        }
    }
    cout<<"Original matrix is:\n";
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
    cout<<"Transpose of the matrix:\n";
    for(int j=0;j<c;j++){
        for(int i=0;i<r;i++){
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
    return 0;

}


